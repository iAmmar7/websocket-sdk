{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Browser",
  "extends": "./base.json",
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "lib": ["ESNext", "DOM"],
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "noEmit": true,
    "noImplicitReturns": true,
    "experimentalDecorators": true
  }
}
